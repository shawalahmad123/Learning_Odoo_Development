<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="report_vehicle">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">    
                <!-- <h1>Testing</h1> -->
                    <br/>
                    Vehicle ID: <t t-esc="doc.vehicle_id"/>
                    <br/>
                    Vehicle Name: <t t-esc="doc.vehicle_name"/>
                    <br/>
                    No. Wheels: <t t-esc="doc.vehicle_wheels"/>
                    <br/>
                    <t t-foreach="doc.vehicle_tires" t-as="tyre">
                        Tyre Name: <t t-esc="tyre.name"/> - Company: <t t-esc="tyre.company"/>
                    </t>
                    <br/>
                    Vehicle Type: <t t-esc="doc.vehicle_type"/>
                    <br/>
                    Vehicle Modal: <t t-esc="doc.vehicle_modal"/>
                    <br/>
                </t>
            </t>
        </t>
    </template>

</odoo>
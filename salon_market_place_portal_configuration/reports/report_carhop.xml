<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="report_carhops">
        <t t-call="web.html_container">

                <div class="page" >





                

                           <img t-att-src="'data:image/png;base64,'+doc.get_carhop_image()" style="position:absolute;left:8;top:0;width:400px;height:400px;"/>




                            <img src="https://restaurant.waiteroo.com/waiteroo_market_place_portal_configuration/static/description/carhop.jpg" />





                </div>

        </t>
    </template>


    <template id="report_carhop">
        <t t-call="web.html_container">

            <t t-foreach="docs" t-as="doc" >

                <t t-call="waiteroo_market_place_portal_configuration.report_carhops" />
            </t>

        </t>
    </template>





    <template id="report_carhops_qr">
        <t t-call="web.html_container">

                <div class="page" >




                           <img t-att-src="'data:image/png;base64,'+doc.get_carhop_image()" style="width:400px;height:400px;"/>





                </div>

        </t>
    </template>


    <template id="report_carhop_qr">
        <t t-call="web.html_container">

            <t t-foreach="docs" t-as="doc">

                <t t-call="waiteroo_market_place_portal_configuration.report_carhops_qr" />
            </t>

        </t>
    </template>




</odoo>
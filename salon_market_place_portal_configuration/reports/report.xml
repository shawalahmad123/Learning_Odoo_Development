<?xml version="1.0" encoding="utf-8" ?>
<odoo>
<!-- QWeb Reports -->
<!-- NAME & FILE_NAME should be the same -->
        <report
            id="report_restaurant_carhop"
            model="res.partner"
            report_type="qweb-pdf"
            string="Print Carhop Banner"
            name="waiteroo_market_place_portal_configuration.report_carhop"
            file="waiteroo_market_place_portal_configuration.report_carhop"
            print_report_name="'%s - Carhop' % object.name"
        />

           <report
            id="report_restaurant_carhop_qr"
            model="res.partner"
            report_type="qweb-pdf"
            string="Print Carhop QR"
            name="waiteroo_market_place_portal_configuration.report_carhop_qr"
            file="waiteroo_market_place_portal_configuration.report_carhop_qr"
            print_report_name="'%s - Carhop QR' % object.name"
        />


    </odoo>
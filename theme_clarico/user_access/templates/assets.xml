<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--======================== Inclusion of js Files ======================-->
    <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="script[last()]" position="after">
             <script type="text/javascript" src="/user_access/static/src/js/frontend/user_access.js"/>
        </xpath>
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/user_access/static/src/scss/user_access.scss"/>
        </xpath>
    </template>
</odoo>